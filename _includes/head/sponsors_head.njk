{% extends "head/_head.njk" %}

{% block headMeta %}
<!-- SEO Meta -->
<meta name="description" content="Thank you to the sponsors of Hudson Stingers Robotics, FRC Team 4295 at Hudson High School in Lufkin, TX. Learn how your business can support STEM education and gain visibility by sponsoring our robotics team.">
<meta name="keywords" content="Hudson Stingers sponsors, FRC Team 4295 sponsors, Hudson High School robotics sponsorship, STEM education sponsorship Texas, FIRST Robotics Competition sponsors">

<link rel="canonical" href="https://www.hudsonrobotics4295.com/sponsors/">
<meta name="robots" content="index, follow">

<!-- Open Graph -->
<meta property="og:type" content="website">
<meta property="og:title" content="Sponsors | Hudson Stingers Robotics FRC Team 4295">
<meta property="og:description" content="Support Hudson Stingers Robotics, FRC Team 4295. See our current sponsors and learn how your business can join in supporting STEM education.">
<meta property="og:url" content="https://www.hudsonrobotics4295.com/sponsors/">
<meta property="og:image" content="https://www.hudsonrobotics4295.com/images/team-photo.jpg">

<!-- Twitter Card -->
<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="Sponsors | Hudson Stingers Robotics FRC Team 4295">
<meta name="twitter:description" content="Thank you to our sponsors! Learn how to support Hudson Stingers Robotics, FRC Team 4295.">
<meta name="twitter:image" content="https://www.hudsonrobotics4295.com/images/team-photo.jpg">

<style>
  .sponsor-logo {
    background-color: #1e1e1e;
    padding: 20px;
    border-radius: 5px;
    margin-bottom: 20px;
  }
</style>

<!-- Structured Data: Sponsors & Offers -->
<script type="application/ld+json">
{
  "@context":"https://schema.org",
  "@type":"Organization",
  "name":"Hudson Stingers Robotics",
  "alternateName":"Hudson High School FRC Team 4295",
  "url":"https://www.hudsonrobotics4295.com/",
  "logo":"https://www.hudsonrobotics4295.com/images/logo.png",
  "sponsor":[
    {"@type":"Organization","name":"Lockheed Martin"},
    {"@type":"Organization","name":"Optimum"},
    {"@type":"Organization","name":"Kasye's Kreations"},
    {"@type":"Organization","name":"Southern Magic Travel"},
    {"@type":"Organization","name":"East Texas Glass"}
  ],
  "makesOffer":{
    "@type":"Offer",
    "name":"Hudson Stingers Robotics Sponsorship",
    "description":"Support Hudson Stingers Robotics, FRC Team 4295, and gain visibility in the Hudson/Lufkin community while promoting STEM education.",
    "url":"https://www.hudsonrobotics4295.com/sponsors/"
  }
}
</script>
{% endblock %}